<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AdditionService WSDL service -->

  <service name="Addition" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://axis1.com"/>
      <parameter name="wsdlServiceElement" value="AdditionService"/>
      <parameter name="schemaQualified" value="http://axis1.com"/>
      <parameter name="wsdlServicePort" value="Addition"/>
      <parameter name="className" value="com.axis1.AdditionSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Addition"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://axis1.com" returnQName="retNS:addReturn" xmlns:retNS="http://axis1.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://axis1.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://axis1.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="add"/>

  </service>
</deployment>
